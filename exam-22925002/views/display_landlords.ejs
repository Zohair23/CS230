<h1>Landlords</h1>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>First Names</th>
            <th>Surname</th>
            <th>Phone Number</th>
            <th>Email Address</th>
            <th>Address Line 1</th>
            <th>Town</th>
            <th>County</th>
            <th>Eircode</th>
            <th>Date of Birth</th>
            <th>Rent Permission</th>
            <th>Tenant Contact Permission</th>
        </tr>
    </thead>
    <tbody>
        <% landlords.forEach(landlord => { %>
            <tr>
                <td><%= landlord._id %></td>
                <td><%= landlord.title %></td>
                <td><%= landlord.firstNames %></td>
                <td><%= landlord.surname %></td>
                <td><%= landlord.phoneNumber %></td>
                <td><%= landlord.emailAddress %></td>
                <td><%= landlord.homeAddress.addressLine1 %></td>
                <td><%= landlord.homeAddress.town %></td>
                <td><%= landlord.homeAddress.county %></td>
                <td><%= landlord.homeAddress.eircode %></td>
                <td><%= landlord.dateOfBirth.toISOString().substr(0, 10) %></td>
                <td><%= landlord.rentPermission %></td>
                <td><%= landlord.tenantContactPermission %></td>
            </tr>
        <% }) %>
    </tbody>
</table>