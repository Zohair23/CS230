<h1>Tenants</h1>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>First Names</th>
            <th>Surname</th>
            <th>Phone Number</th>
            <th>Email Address</th>
            <th>Address Line 1</th>
            <th>Town</th>
            <th>County</th>
            <th>Eircode</th>
        </tr>
    </thead>
    <tbody>
        <% tenants.forEach(tenant => { %>
            <tr>
                <td><%= tenant._id %></td>
                <td><%= tenant.title %></td>
                <td><%= tenant.firstNames %></td>
                <td><%= tenant.surname %></td>
                <td><%= tenant.phoneNumber %></td>
                <td><%= tenant.emailAddress %></td>
                <td><%= tenant.homeAddress.addressLine1 %></td>
                <td><%= tenant.homeAddress.town %></td>
                <td><%= tenant.homeAddress.county %></td>
                <td><%= tenant.homeAddress.eircode %></td>
            </tr>
        <% }) %>
    </tbody>
</table>