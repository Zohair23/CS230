<h1>Create New Landlord</h1>

<form id="newLandlordForm" action="/landlords" method="post">
    <label for="title">Title:</label>
    <select name="title" id="title">
        <option value="Mx">Mx</option>
        <option value="Ms">Ms</option>
        <option value="Mr">Mr</option>
        <option value="Mrs">Mrs</option>
        <option value="Miss">Miss</option>
        <option value="Dr">Dr</option>
        <option value="Prof">Prof</option>
    </select><br><br>
    <label for="firstNames">First Names:</label>
    <input type="text" name="firstNames" id="firstNames"><br><br>
    <label for="surname">Surname:</label>
    <input type="text" name="surname" id="surname"><br><br>
    <label for="phoneNumber">Phone Number:</label>
    <input type="text" name="phoneNumber" id="phoneNumber"><br><br>
    <label for="emailAddress">Email Address:</label>
    <input type="email" name="emailAddress" id="emailAddress"><br><br>
    <label for="addressLine1">Address Line 1:</label>
    <input type="text" name="homeAddress.addressLine1" id="addressLine1"><br><br>
    <label for="addressLine2">Address Line 2:</label>
    <input type="text" name="homeAddress.addressLine2" id="addressLine2"><br><br>
    <label for="town">Town:</label>
    <input type="text" name="homeAddress.town" id="town"><br><br>
    <label for="county">County:</label>
    <input type="text" name="homeAddress.county" id="county"><br><br>
    <label for="eircode">Eircode:</label>
    <input type="text" name="homeAddress.eircode" id="eircode"><br><br>
    <label for="dateOfBirth">Date of Birth:</label>
    <input type="date" name="dateOfBirth" id="dateOfBirth"><br><br>
    <label for="rentPermission">Rent Permission:</label>
    <select name="rentPermission" id="rentPermission">
        <option value="Y">Yes</option>
        <option value="N">No</option>
    </select><br><br>
    <label for="tenantContactPermission">Tenant Contact Permission:</label>
    <select name="tenantContactPermission" id="tenantContactPermission">
        <option value="Y">Yes</option>
        <option value="N">No</option>
    </select><br><br>
    <button type="submit">Create Landlord</button>
</form>

